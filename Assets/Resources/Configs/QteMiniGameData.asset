  _programText: "using System.Collections.Generic;\r\nusing System.Collections.ObjectModel;\r\nusing
    System.Linq;\r\nusing Configs;\r\nusing UniRx;\r\n\r\nnamespace UI.Programs.TrelloMiniGame.ViewModel\r\n{\r\n   
    public class TrelloColumnViewModel : ITrelloColumnViewModel\r\n    {\r\n       
    public string TitleColumn { get; }\r\n        public IReactiveProperty<ReadOnlyCollection<ITrelloTaskViewModel>>
    Tasks => _tasksProp;\r\n\r\n        private readonly ReactiveProperty<ReadOnlyCollection<ITrelloTaskViewModel>>
    _tasksProp = new ();\r\n        private readonly List<TrelloTaskViewModel> _tasks;\r\n       
    private readonly int _columnNumber;\r\n\r\n        public TrelloColumnViewModel(TrelloMiniGameData.ColumnData
    data, TrelloMiniGameViewModel miniGameViewModel, int columnNumber)\r\n       
    {\r\n            _tasks = data.Tasks.Select(taskData => new TrelloTaskViewModel(taskData,
    miniGameViewModel.OnClickTaskHandler, miniGameViewModel.OnChangeTaskColumn, columnNumber)).ToList();\r\n           
    _tasksProp.Value = _tasks.Cast<ITrelloTaskViewModel>().ToList().AsReadOnly();\r\n           
    _columnNumber = columnNumber;\r\n        }\r\n\r\n        public bool TryRemoveTask(TrelloTaskViewModel
    taskViewModel)\r\n        {\r\n            if(!_tasks.Contains(taskViewModel))\r\n           
    {\r\n                return false;\r\n            }\r\n\r\n            _tasks.Remove(taskViewModel);\r\n           
    _tasksProp.Value =_tasks.Cast<ITrelloTaskViewModel>().ToList().AsReadOnly();\r\n           
    return true;\r\n        }\r\n\r\n        public bool TryAddTask(TrelloTaskViewModel
    taskViewModel)\r\n        {\r\n            if(_tasks.Contains(taskViewModel))\r\n           
    {\r\n                return false;\r\n            }\r\n\r\n            _tasks.Add(taskViewModel);\r\n           
    taskViewModel.SetColumnNumber(_columnNumber);\r\n            _tasksProp.Value
    =_tasks.Cast<ITrelloTaskViewModel>().ToList().AsReadOnly();\r\n            return
    true;\r\n        }\r\n    }\r\n}"
